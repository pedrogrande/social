<h1>Listing businesses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Industry</th>
      <th>Address</th>
      <th>Suburb</th>
      <th>Postcode</th>
      <th>Country</th>
      <th>Phone</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @businesses.each do |business| %>
      <tr>
        <td><%= business.name %></td>
        <td><%= business.industry %></td>
        <td><%= business.address %></td>
        <td><%= business.suburb %></td>
        <td><%= business.postcode %></td>
        <td><%= business.country %></td>
        <td><%= business.phone %></td>
        <td><%= business.email %></td>
        <td><%= link_to 'Show', business %></td>
        <td><%= link_to 'Edit', edit_business_path(business) %></td>
        <td><%= link_to 'Destroy', business, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business', new_business_path %>
